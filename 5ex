#include<iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "ru");
    const int N = 100;


    int array[N];

    for (int i = 0; i < N; ++i) {
        array[i] = i;
    }

    cout << "Элементы массива с четными номерами: ";
    for (int i = 0; i < N; i += 2) {
        cout << array[i] << " ";
    }

    cout << endl;

    return 0;
}
